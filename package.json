{
  "name": "hyper-ts",
  "version": "0.5.3",
  "description": "hyper-ts description",
  "files": [
    "lib",
    "es6"
  ],
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint -p .",
    "jest": "jest",
    "mocha": "TS_NODE_CACHE=false mocha -r ts-node/register test/*.ts",
    "prettier": "prettier --no-semi --single-quote --print-width 120 --parser typescript --list-different \"{src,test}/**/*.ts\"",
    "fix-prettier": "prettier --no-semi --single-quote --print-width 120 --parser typescript --write \"{src,test}/**/*.ts\"",
    "test": "npm run lint && npm run prettier && npm run dtslint && npm run jest && npm run docs",
    "clean": "rm -rf ./lib ./es6",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.build-es6.json",
    "postbuild": "import-path-rewrite && prettier --write \"./{lib,es6}/**/*.ts\"",
    "prepare": "npm run build",
    "dtslint": "dtslint dtslint",
    "docs": "docs-ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcanti/hyper-ts.git"
  },
  "author": "Giulio Canti <giulio.canti@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanti/hyper-ts/issues"
  },
  "homepage": "https://github.com/gcanti/hyper-ts",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.43",
    "@types/benchmark": "^1.0.31",
    "@types/express": "^4.17.2",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.4",
    "@types/qs": "^6.9.1",
    "benchmark": "^2.1.4",
    "docs-ts": "^0.3.4",
    "dtslint": "github:gcanti/dtslint",
    "fp-ts": "^2.5.1",
    "fp-ts-routing": "^0.5.3",
    "import-path-rewrite": "github:gcanti/import-path-rewrite",
    "io-ts": "^2.1.1",
    "io-ts-types": "^0.5.5",
    "jest": "^25.1.0",
    "mocha": "^7.0.1",
    "prettier": "^1.19.1",
    "qs": "^6.9.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "tslint-config-standard": "^9.0.0",
    "typescript": "^3.8.2"
  },
  "peerDependencies": {
    "express": "^4.17.1",
    "aws-lambda": "^1.0.5"
  },
  "dependencies": {
    "fp-ts": "^2.5.1"
  },
  "tags": [
    "typescript",
    "express",
    "middleware",
    "fp-ts",
    "io-ts",
    "functional-programming",
    "hyper"
  ],
  "keywords": [
    "typescript",
    "express",
    "middleware",
    "fp-ts",
    "io-ts",
    "functional-programming",
    "hyper"
  ]
}
