{
  "name": "hyper-ts",
  "version": "0.6.0",
  "description": "Type safe middleware architecture for HTTP servers",
  "files": [
    "lib",
    "es6"
  ],
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint -p .",
    "jest": "jest",
    "mocha": "TS_NODE_CACHE=false mocha -r ts-node/register test/*.ts",
    "prettier": "prettier --no-semi --single-quote --print-width 120 --parser typescript --list-different \"{src,test}/**/*.ts\"",
    "fix-prettier": "prettier --no-semi --single-quote --print-width 120 --parser typescript --write \"{src,test}/**/*.ts\"",
    "test": "npm run lint && npm run prettier && npm run dtslint && npm run jest && npm run docs",
    "clean": "rm -rf ./lib ./es6",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.build.json && tsc -p ./tsconfig.build-es6.json",
    "postbuild": "import-path-rewrite && prettier --write \"./{lib,es6}/**/*.ts\"",
    "prepublish": "npm run build",
    "dtslint": "dtslint dtslint",
    "docs": "docs-ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gcanti/hyper-ts.git"
  },
  "author": "Giulio Canti <giulio.canti@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gcanti/hyper-ts/issues"
  },
  "homepage": "https://github.com/gcanti/hyper-ts",
  "devDependencies": {
    "@types/benchmark": "^1.0.31",
    "@types/express": "4.11.0",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.11.6",
    "@types/qs": "^6.5.2",
    "benchmark": "^2.1.4",
    "docs-ts": "^0.4.0",
    "dtslint": "github:gcanti/dtslint",
    "fp-ts": "^2.8.4",
    "fp-ts-routing": "^0.4.1",
    "import-path-rewrite": "github:gcanti/import-path-rewrite",
    "io-ts": "^2.2.11",
    "io-ts-types": "^0.5.11",
    "jest": "^24.8.0",
    "mocha": "^5.2.0",
    "prettier": "^1.19.1",
    "qs": "^6.7.0",
    "ts-jest": "^24.0.2",
    "ts-node": "3.3.0",
    "tslint": "^5.14.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "fp-ts": "^2.8.4",
    "express": "^4.16.2"
  },
  "tags": [
    "typescript",
    "express",
    "middleware",
    "fp-ts",
    "io-ts",
    "functional-programming",
    "hyper"
  ],
  "keywords": [
    "typescript",
    "express",
    "middleware",
    "fp-ts",
    "io-ts",
    "functional-programming",
    "hyper"
  ]
}
