{
  "compilerOptions": {
    "module": "commonjs",
    "strict": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "target": "es5",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "lib": [
      "es6",
      "dom"
    ]
  }
}
